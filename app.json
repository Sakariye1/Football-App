{
  "expo": {
    "name": "GOALZY",
    "slug": "goalzy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/logo2.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "“GOALZY\" app provides you the latest live table, fixtures and stats. You can follow Europe’s top 4 leagues.\nData is fetched from football-data.org Api.\n"
  }
}
